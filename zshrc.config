# Use lf to switch directories and bind it to ctrl-o
lfcd () {
    tmp="$(mktemp)"
    lf -last-dir-path="$tmp" "$@"
    if [ -f "$tmp" ]; then
        dir="$(cat "$tmp")"
        rm -f "$tmp"
        [ -d "$dir" ] && [ "$dir" != "$(pwd)" ] && cd "$dir"
    fi
}
bindkey -s '^o' 'lfcd\n'

bindkey -s '^n' 'ifconfig\n'
# Function to launch nnn and return to the last directory
n() {
    # Prevent nesting of nnn in subshells
    [ "${NNNLVL:-0}" -eq 0 ] || {
        echo "nnn is already running"
        return
    }

    # Set the temporary file for directory change
    export NNN_TMPFILE="${XDG_CONFIG_HOME:-$HOME/.config}/nnn/.lastd"

    # Default options for nnn
    local DEFAULT_OPTS="-de"

    # Launch nnn with default options and user-provided arguments
    command nnn $DEFAULT_OPTS "$@"

    # Change to the directory recorded by nnn
    if [ -f "$NNN_TMPFILE" ]; then
        . "$NNN_TMPFILE"
        rm -f "$NNN_TMPFILE"
    fi
}

# Sync subshell $PWD after nnn quits
nnn_cd() {
    if [ -n "$NNN_PIPE" ]; then
        printf "%s\0" "0c${PWD}" > "${NNN_PIPE}" &
    fi
}

# Add trap to handle subshell directory syncing
trap nnn_cd EXIT



